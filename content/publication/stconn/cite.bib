@article{beigi_quantum_2019,
	title = {Quantum {Speedup} {Based} on {Classical} {Decision} {Trees}},
	url = {http://arxiv.org/abs/1905.13095},
	abstract = {Lin and Lin have recently shown how starting with a classical query algorithm (decision tree) for a function, we may find upper bounds on its quantum query complexity. More precisely, they have shown that given a decision tree for a function \$f:{\textbackslash}\{0,1{\textbackslash}\}{\textasciicircum}n{\textbackslash}to[m]\$ whose input can be accessed via queries to its bits, and a guessing algorithm that predicts answers to the queries, there is a quantum query algorithm for \$f\$ which makes at most \$O({\textbackslash}sqrt\{GT\})\$ quantum queries where \$T\$ is the depth of the decision tree and \$G\$ is the maximum number of mistakes of the guessing algorithm. In this paper we give a simple proof of and generalize this result for functions \$f:[{\textbackslash}ell]{\textasciicircum}n {\textbackslash}to [m]\$ with non-binary input as well as output alphabets. Our main tool for this generalization is non-binary span program which has recently been developed for non-binary functions, and the dual adversary bound. As applications of our main result we present several quantum query upper bounds, some of which are new. In particular, we show that topological sorting of vertices of a directed graph \${\textbackslash}mathcal\{G\}\$ can be done with \$O(n{\textasciicircum}\{3/2\})\$ quantum queries in the adjacency matrix model. Also, we show that the quantum query complexity of the maximum bipartite matching is upper bounded by \$O(n{\textasciicircum}\{3/4\}{\textbackslash}sqrt m + n)\$ in the adjacency list model.},
	urldate = {2020-02-25},
	journal = {arXiv:1905.13095 [quant-ph]},
	author = {Beigi, Salman and Taghavi, Leila},
	month = sep,
	year = {2019},
	note = {arXiv: 1905.13095},
	keywords = {Computer Science - Data Structures and Algorithms, Quantum Physics},
	file = {arXiv Fulltext PDF:/Users/rtealw/Zotero/storage/WGI7GZTE/Beigi and Taghavi - 2019 - Quantum Speedup Based on Classical Decision Trees.pdf:application/pdf;arXiv.org Snapshot:/Users/rtealw/Zotero/storage/PL798HH9/1905.html:text/html}
